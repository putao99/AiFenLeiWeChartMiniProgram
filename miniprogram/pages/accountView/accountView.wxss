.container{
  position: absolute;
  background-color: #fff;
  padding: 10px 10px 0 10px;
  top: 0;
  left: 0;
  right: 0;
  z-index: 100;
}

.account-hd{
  position: relative;
  padding: 20px;
  /* background: -webkit-linear-gradient(left bottom, #ff8570,#ff9288);
  background: linear-gradient(left bottom, #ff8570,#ff9288);
  box-shadow: 1px 0 3px 0 rgba(39, 54, 78, 0.12);
  border-radius: 6px; */
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  display: flex;
  flex-direction: column;
  
}

.score-current,.score-in,score-out{
  display: flex;
  flex-direction: column;
}

.score-sub{
  display: flex;
  flex-direction: row;
}
.score-in{
  padding-right: 20px;
}
.score-out{
  border-left: 1rpx solid #fff;
  padding-left: 20px;
}

.score-tips{font-size: 12px;color: #fff;}
.score-tips-min{font-size: 10px;}
.score{font-size: 20px;font-weight: 700;color: #fff;}
.score-min{font-size: 16px;}

.cash{
  position: absolute;
  top: 20px;
  right: 20px;
  color: #fff;
  font-size: 13px;
  padding: 0 10px 0 20px;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-position: 5px center;
  background-size: 12px auto;
  border: 1rpx solid #fff;
  border-radius: 1000px;
}

.filter-bar{
  display: flex;
  flex-direction: row;
  justify-items: center;
  align-items: center;
  padding: 10px;
  border-bottom: 1rpx solid rgba(0,0,0,.1);
}

.filter-bar .filter-item{
  display: flex;
  flex-direction: row;
  justify-content: center;
  justify-items: center;
  align-items: center;
  position: relative;
  flex: 1;
  align-self: center;
  font-size: 14px;
  text-align: center;
  color: #444;
}

.filter-item__icon{
  position: relative;
  width: 0;
  height: 0;
  border: 6px solid #444;
  transition:  all 0.3s linear;
  transform: rotate3d(1,0,0,0);
  border-bottom-color: transparent;
  border-left-color: transparent;
  border-right-color: transparent;
  margin-left: 5px;
}

.filter-item__icon.up{
  top: -3px;
  transform: rotate3d(0,0,1,180deg);
}

.filter-item__icon.down{
  top: 3px;
  transform: rotate3d(0,0,1,0deg);
  border-top-color: #78C9CC;
}

.filter-bd{
  position: absolute;
  top: 185px;
  left: 0;
  right: 0;
  z-index: 101;
  font-size: 14px;
  overflow: hidden;
  background-color: #fff;
  box-shadow: 1px 0 3px 0 rgba(39, 54, 78, 0.12);
}
.filter-bd-sv{

  border-bottom: 1rpx solid rgba(0,0,0,.1);
  transition: all .2s linear;
}
.filter-bd .item-wrap{
  position: relative;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  padding: 8px 20px 8px 0;
  border-bottom: 1rpx solid rgba(0,0,0,.1);
  margin-left: 20px;
  font-size: 13px;
}
.filter-bd .item-wrap:last-child{
  border-bottom:none;
}

.filter-bd .item-wrap .item-name{
  flex: 1;
}
.filter-bd .item-wrap .item-name.selected{
  color: #78C9CC;
}

.no-data{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 14px;
  color: rgba(0,0,0,.5);

  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}

.subdata-wrap{
  background-color: #eee;
  margin: 10px 10px 0 10px;
}

.item-subdata{
  display: flex;
  flex-direction: row;
  margin-bottom: 10px;
  background-color: #fff;
  border-radius: 3px;
  padding: 10px 15px;
}
.item-subdata .item-subdata-thumb{
  display: flex;
  margin-right: 15px;
  justify-content: center;
  align-items: center;
}
.item-subdata .item-subdata-wrap{
  flex: 1;
  display: flex;
  flex-direction: column;
  overflow: hidden;
}
.item-subdata-title,.item-subdata-time{
  flex: 1;
  min-width: 0;
  position: relative;
}

.item-subdata-title-txt{
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.item-subdata-content,.item-subdata-desc{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.item-subdata-content{
  font-size: 16px;
  color: #444;
  font-weight: 700;
}
.item-subdata-content .item-subdata-score{
  color: #78C9CC;
  font-weight: normal;
}
.item-subdata-desc{
  font-size: 12px;
  color: rgba(0,0,0,.5);
}

/* loading... */
.loading-wrapper{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: -10;
  background-color:transparent;
  display: flex;
  justify-content: center;
  align-items: center;
  opacity: 0;
  transition: all 0.2s linear;
}
.loading-wrapper.active{
  opacity: 1;
  background-color: rgba(0,0,0,.5);
  z-index: 10;
}
.loading-main{
	display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.loading-content{
	font-size: 13px;
	color: #fff;
  
}
.loading{
  display: inline-block;
  width: 48px;
  height: 48px;
  border-radius: 50%;
  border:3px solid hsla(185, 100%, 62%, 0.2);
  border-top-color: #3cefff;
  animation: spin 1s linear infinite;
}
@keyframes spin{
  to{
    transform: rotate(360deg);
  }
}
/* loading... END */


