/* loading... */
.loading-wrapper{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 999;
  background-color: rgba(0,0,0,.8);
  display: flex;
  justify-content: center;
  align-items: center;
}
.loading-main{
	display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.loading-content{
	font-size: 13px;
	color: #fff;
  margin-top: 5px;
}
.loading{
  display: inline-block;
  width: 48px;
  height: 48px;
  border-radius: 50%;
  border:3px solid hsla(185, 100%, 62%, 0.2);
  border-top-color: #3cefff;
  animation: spin 1s linear infinite;
}
@keyframes spin{
  to{
    transform: rotate(360deg);
  }
}
/* loading... END */

